{
  "name": "weaveit",
  "version": "0.0.1",
  "description": "A solana package to generate tutorial videos from text and audio",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "start": "node dist/cli.js",
    "test": "node dist/testVideoGeneration.js",
    "dc": "node defchrome.js",
    "server": "node dist/server.js",
    "dev": "ts-node src/server.ts",
    "voice": "npx ts-node src/cli.ts analyze -f tester.ts --voice",
    "video": "npx ts-node src/cli.ts analyze -f tutorial.ts --voice --video"
  },
  "keywords": [
    "tutorial",
    "video",
    "sdk",
    "text-to-video",
    "text-to-speech",
    "node",
    "canvas",
    "ffmpeg",
    "tts"
  ],
  "author": "Loganthewise",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:lawalabdulrazaq/weaveit.git"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.10.2",
    "@solana/spl-token": "^0.4.13",
    "@solana/web3.js": "^1.98.0",
    "@types/three": "^0.176.0",
    "axios": "^1.8.4",
    "bs58": "^6.0.0",
    "canvas": "^3.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^5.1.0",
    "ffmpeg": "^0.0.4",
    "ffmpeg-static": "^5.2.0",
    "fluent-ffmpeg": "^2.1.3",
    "gl": "^8.1.6",
    "mongoose": "^8.14.2",
    "multer": "^1.4.5-lts.2",
    "node-gtts": "^2.0.2",
    "openai": "^4.92.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "three": "^0.176.0",
    "three-stdlib": "^2.36.0",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.1",
    "@types/fluent-ffmpeg": "^2.1.27",
    "@types/gl": "^6.0.5",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.14.0",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
